
 CREATE DATABASE NLDBD;

CREATE TABLE DatosNuevoLeon (
    IdNl varchar(5),
    Indicador varchar(255),
    Fuente varchar(255),
    Unidad varchar(255),
    Periodicidad(255),
    ABaseValor varchar(255),
    AActualvalor varchar(255),
    Meta int,
    Tema varchar(255),
    AreaDeOportunidades varchar(255)
);

CREATE TABLE Municipio (
    PMunicipal varchar(255),
    Contacto varchar(255),
    Nombre varchar(255),
);

